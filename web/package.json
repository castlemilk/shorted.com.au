{
  "name": "shorted",
  "version": "v0.2.2-625-gcf24c6f0-dirty",
  "private": true,
  "type": "module",
  "moduleResolution": "Node",
  "scripts": {
    "prebuild": "npm run bump-version",
    "build": "next build",
    "postbuild": "bash scripts/warm-cache-on-deploy.sh || true",
    "build:analyze": "ANALYZE=true next build",
    "lighthouse": "node scripts/lighthouse-ci.js",
    "perf:check": "npm run build && npm run start & sleep 5 && npm run lighthouse",
    "dev": "APP_VERSION=`git describe --tags --always --dirty` next dev -p 3020",
    "bump-version": "bash ./scripts/bump-version.sh",
    "lint": "next lint --max-warnings=1000",
    "start": "next start -p 3020",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:cache": "jest --testPathPattern='cache|warm-cache|page-server'",
    "test:cache:manual": "bash scripts/test-cache.sh",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:full": "bash ../scripts/run-e2e-tests.sh",
    "test:seed": "tsx scripts/test-db-setup.ts",
    "sync-short-data": "node --loader ts-node/esm scripts/sync-short-data.ts",
    "db:setup": "tsx scripts/db-setup.ts",
    "db:seed": "tsx scripts/db-setup.ts --seed",
    "db:sync-asic": "tsx scripts/sync-asic-data.ts",
    "db:sync-asic:week": "tsx scripts/sync-asic-data.ts --days 7",
    "db:sync-asic:month": "tsx scripts/sync-asic-data.ts --days 30",
    "db:sync-market": "tsx scripts/sync-market-data.ts",
    "db:sync-market:week": "tsx scripts/sync-market-data.ts --days 7",
    "db:sync-market:month": "tsx scripts/sync-market-data.ts --days 30",
    "db:sync-market:backfill": "tsx scripts/sync-market-data.ts --backfill --days 90",
    "db:sync-all": "npm run db:sync-asic && npm run db:sync-market",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:migrate": "supabase db push",
    "algolia:sync": "tsx scripts/sync-algolia.ts"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/test/setup.ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^@/auth$": "<rootDir>/src/test/__mocks__/@/auth.js",
      "^next-auth/react$": "<rootDir>/src/test/__mocks__/next-auth-react.js",
      "^@/lib/utils$": "<rootDir>/src/@/lib/utils.ts",
      "^@/(.*)$": "<rootDir>/src/@/$1",
      "^~/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "**/scripts/**/*.test.ts",
      "**/src/**/*.test.ts",
      "**/src/**/*.test.tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "tsconfig": {
            "jsx": "react-jsx"
          },
          "isolatedModules": true
        }
      ]
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@bufbuild|@connectrpc|next-auth|@auth|firebase|@visx|d3-array|d3-scale|d3-shape|d3-time|d3-format|@upstash|uncrypto)/)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/src/gen/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/test/**/*",
      "!src/gen/**/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 40,
        "lines": 40,
        "statements": 40
      }
    }
  },
