# Stock Price Ingestion Service - Environment Variables
# Copy this file to .env and fill in your values

# Database connection (required)
DATABASE_URL=postgresql://admin:password@localhost:5438/shorts

# Alpha Vantage API Key (required for primary provider)
# Get your free key from: https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=your_api_key_here

# GCP Configuration (for production)
GCP_PROJECT=shorted-dev-aba5688f
ENVIRONMENT=development

# Provider Selection (optional - defaults shown)
PRIMARY_PROVIDER=alpha_vantage
FALLBACK_PROVIDER=yahoo_finance

# Data ingestion settings (optional)
DAYS_BACK=730  # Default: 2 years of historical data

